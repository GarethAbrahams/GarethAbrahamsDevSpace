<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.garethabrahams.garethabrahamsdevspace">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fox"
        android:label="Gareth Dev Space"
        android:roundIcon="@drawable/fox"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".CalcActivity" />
        <activity android:name=".AboutMeActivity" />
        <activity android:name=".MenuActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Gareth Dev Space"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>